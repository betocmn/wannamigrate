.content
    &.myaccount
        width 960px
        margin 0 auto 58px
        padding 58px 0 0 0
        overflow hidden

        .title
            display block
            padding 0 0 50px
            text-transform uppercase
            font-size 20px
            fontbd()
            font-weight normal
            color #68347e

        .interactions
