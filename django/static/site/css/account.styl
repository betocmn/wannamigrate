                    tr
                        // height 45px

                        &.service
                            width 25%

                        &.professional
                            width 42%

                        &.date
                            width 9%

                        &.total
                            width 9%

                        &.status
                            width 11%
                            text-align center

                        th
                            padding 0 0 5px 10px
                            fontsb()
                            font-size 12px
                            color #777
                            text-align left

                        td
                            height 40px
                            padding 0 0 0 10px
                            line-height 35px
                            font-size 14px
                            border 4px solid #f9f9f9
                            background-color #fff
                            color #777

                            &.service
                                line-height 16px

                            &.status
                                fontsb()

                                &.paid
                                    color #179217

                                &.waiting
                                    line-height 16px
                                    color #ff8400

            .inbox
                display none
                position relative
                height 800px
                padding 0 20px 0 38px

                .title
                    display none

                .box-menu
                    position absolute
                    top 79px
                    left 0
                    width 150px
                    border-top 1px solid #e1e1e1

                    .box-item
                        display block
                        size 150px 50px

                        .box-link
                            display block
                            size 150px 50px
                            padding 0 0 0 35px
                            box()
                            line-height 50px
                            font-size 14px
                            fontsb()
                            color #888
                            text-decoration none

                            &.active
                                fontbd()
                                background-color #fff
                                color #68347e
                                border-right 6px solid #ffc600

                .box-top
                    position absolute
                    top 0
