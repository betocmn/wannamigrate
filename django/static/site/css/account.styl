                top -37px
                size 960px 35px
                margin 0 auto

                .interaction-item
                    float left
                    display block
                    width 138px
                    margin 0 2px 0
                    text-align center

                    &:first-child
                        margin-left 0

                    .interaction-link
                        display block
                        height 35px
                        line-height 35px
                        text-decoration none
                        border-left 1px solid #d7d7d7
                        border-top 1px solid #d7d7d7
                        border-right 1px solid #d7d7d7
                        border-radius 5px 5px 0 0
                        font-size 14px
                        background-color #eaeaea
                        color #888

                        &.active
                            position relative
                            background-color #fafafa
                            border-bottom 1px solid #fafafa
                            color #68347e
                            pointer-events none

