                    .dialogues
                        size 100% auto
                        padding-top 15px
                        border-left 1px solid #e1e1e1
                        overflow hidden

                        .dialogue
                            position relative
                            float right
                            width 715px
                            padding 15px 20px
                            box()
                            margin-bottom 15px
                            border 1px solid #b7b7b7
                            border-radius(5px)
                            background-color #fff

                            &.recent
                                .when
                                    color #68347e

                            &.odd
                                background-color #f1f1f1
                                border transparent

                                .when
                                    text-transform uppercase

                            &.even
                                background-color #fff
                                border transparent

                                .when
                                    text-transform uppercase

