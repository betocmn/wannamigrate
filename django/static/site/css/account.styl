                                    p
                                        position relative
                                        display block
                                        size 105px 20px
                                        font-size 14px
                                        color #68347e

                                        input
                                            position absolute
                                            left 0
                                            top 0
                                            size 105px 20px
                                            cursor pointer
                                            opacity 0

                                label
                                    display block
                                    padding 0 0 5px 0
                                    font-size 14px
                                    color #888

                                input,
                                textarea
                                    width 70%
                                    font-size 14px
                                    color #888

                                textarea
                                    display block
                                    padding 10px 10px
                                    resize none
                                    box()
                                    border 1px solid #d0b5db
                                    border-radius(4px)
                                    background-color #fff

